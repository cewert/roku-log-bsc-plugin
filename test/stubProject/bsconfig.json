{
  "rootDir": "src",
  "files": [
    "manifest",
    "source/**/*.*",
    "components/**/*.*"
  ],
  "plugins": [
    "../../dist/plugin.js"
  ],
  "autoImportComponentScript": true,
  "createPackage": false,
  "stagingFolderPath": "/tmp/out",
  "retainStagingFolder": true,
  "rooibos": {
    "isRecordingCodeCoverage": false,
    "testsFilePattern": null
  },
  "rokuLog": {
    "strip": true,
    "insertPkgPath": true
  }
}